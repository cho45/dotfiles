#!/bin/sh

if [ -e $HOME/.keychain/*-sh ]; then
	. $HOME/.keychain/*-sh
fi

ssh-add -l > /dev/null 2>&1

if [ $? -ne 0 ]; then
	keychain --quiet --stop all
	keychain --quiet --timeout 30 $HOME/.ssh/id_dsa
	if [ $? -eq 0 ]; then
		. $HOME/.keychain/*-sh
	fi
fi

COMMAND=${0##*/}
PATH=/opt/local/bin:/bin:/sbin:/usr/bin:/usr/sbin:/usr/local/bin:/usr/local/sbin

/usr/bin/env $COMMAND ${@+"$@"}

